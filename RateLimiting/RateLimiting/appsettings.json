{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false, 
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Limit": 100, 
        "Period": "1h" 
      }
    ]
  }
}
